{
  "name": "macoolka-ncow-web",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "author": "fastspeeed@gmail.com",
  "scripts": {
    "build": "rimraf .next && next build",
    "generate:web": "graphql-codegen --config graphql-codegen.yml",
    "start": "next start -p $PORT",
    "dev": "rimraf ./node_modules/.cache/babel-loader && cross-env NODE_ENV=development &&  next -p 2000",
    "reinstall": "rm -rf node_modules/ && yarn install",
    "test": "jest",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only --project tsconfig.server.json server/schema ",
    "postinstall": "npm -s run generate",
    "dxy": "ts-node --project tsconfig.server.json  ./server/data/bin.ts",
    "update": "ts-node ./src/data/updateY.ts",
    "now": "now",
    "migrate": "npm -s run migrate:create && npm -s run migrate:save",
    "migrate:create": "prisma2 migrate save --name 'add-comment-model' --experimental",
    "migrate:save": " prisma2 migrate up --experimental"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "mocoolka",
    "css",
    "functional"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(./src/*|\\.(test|spec))\\.(ts|tsx)$"
  },
